{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [ "config:base"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "enabled": true,
      "matchPackagePatterns": ["*"]
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "baseBranches": ["/release-*/"],
}
